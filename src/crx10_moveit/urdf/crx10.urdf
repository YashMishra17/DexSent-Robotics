    </collision>
    <inertial>
      <mass value="0.5"/>
      <inertia ixx="0.005" ixy="0.0" ixz="0.0" iyy="0.005" iyz="0.0" izz="0.005"/>
    </inertial>
  </link>
  
  <!-- Gripper finger joint (simplified Robotiq 2F) -->
  <joint name="gripper_finger_joint" type="prismatic">
    <parent link="gripper_base"/>
    <child link="gripper_finger_left"/>
    <origin xyz="0 0.02 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="0.0" upper="0.04" effort="100" velocity="0.1"/>
  </joint>
  
  <link name="gripper_finger_left">
    <visual>
      <origin xyz="0 0.025 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.02 0.05 0.06"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin xyz="0 0.025 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.02 0.05 0.06"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
    </inertial>
  </link>
  
  <!-- Right finger (mirrored) -->
  <joint name="gripper_finger_right_joint" type="prismatic">
    <parent link="gripper_base"/>
    <child link="gripper_finger_right"/>
    <origin xyz="0 -0.02 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-0.04" upper="0.0" effort="100" velocity="0.1"/>
    <mimic joint="gripper_finger_joint" multiplier="-1"/>
  </joint>
  
  <link name="gripper_finger_right">
    <visual>
      <origin xyz="0 -0.025 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.02 0.05 0.06"/>
