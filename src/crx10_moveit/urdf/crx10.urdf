      <material name="gray"/>
    </visual>
  </link>
  
  <!-- Gripper mount -->
  <joint name="gripper_mount_joint" type="fixed">
    <parent link="tool_flange"/>
    <child link="gripper_base"/>
    <origin xyz="0 0 0.01" rpy="0 0 0"/>
  </joint>
  
  <link name="gripper_base">
    <visual>
      <geometry>
        <box size="0.05 0.08 0.04"/>
      </geometry>
      <material name="black">
        <color rgba="0.1 0.1 0.1 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.05 0.08 0.04"/>
